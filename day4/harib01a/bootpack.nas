[FORMAT "WCOFF"]
[INSTRSET "i486p"]
[OPTIMIZE 1]
[OPTION 1]
[BITS 32]
	EXTERN	_write_mem8
	EXTERN	_io_hlt
[FILE "bootpack.c"]
[SECTION .text]
	GLOBAL	_HariMain
_HariMain:
	PUSH	EBP
	MOV	EBP,ESP
	PUSH	ESI
	PUSH	EBX
	MOV	ESI,655360
	MOV	EBX,15
L7:
	PUSH	EBX
	DEC	EBX
	PUSH	ESI
	CALL	_write_mem8
	TEST	EBX,EBX
	POP	EAX
	POP	EDX
	JNE	L4
	MOV	EBX,15
L4:
	INC	ESI
	CMP	ESI,720895
	JLE	L7
L8:
	CALL	_io_hlt
	JMP	L8
